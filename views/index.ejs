    <script>
        var deadline = new Date(2022, 1, 5, 0, 0, 0, 0).getTime();
        var curr_time = new Date().getTime();
            setInterval(function fun() {
                var now = new Date().getTime();
                var t = deadline - now;
                var days = Math.floor(t / (1000 * 60 * 60 * 24));
                var hours = Math.floor((t % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
                var minutes = Math.floor((t % (1000 * 60 * 60)) / (1000 * 60));
                var seconds = Math.floor((t % (1000 * 60)) / 1000);
                console.log("hello");
                document.getElementById("regstarts").innerHTML = "Registrations starts in:";
                let str = days;
                str+= days===1? " day  ": " days  ";
                str += hours;
                str+= hours===1? " hour  ": " hours  ";
                str += minutes;
                str+= minutes===1? " minute  ": " minutes  ";
                str += seconds;
                str+= seconds===1? " second  ": " seconds  ";

                document.getElementById("demo").innerHTML = str;
                if (deadline <= curr_time) {
                    document.getElementById('button').style.display = 'block';
                    document.getElementById('regstarts').style.display = 'none'
                    document.getElementById('demo').style.display = 'none'

                }
            }, 1000);
            let btn = document.getElementById('button');
            btn.style.display = "none";
    </script>
